import java.util.*; 
class stringsplit {  
 public static void main(String args[]) { 
String str;
 Scanner sc=new Scanner(System.in); 
System.out.print("Enter string data:"); 
str=sc.next(); 
char ch[]=str.tocharArray(); 
for(int i=0;i<ch.length;i++) 
{
int m=0;
m=m+str.length();
System.out.print(m); 
} 
} 
}
